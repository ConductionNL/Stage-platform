<div class="card border-light shadow-sm">
    <div class="card-body">
        <h2 class="h5">{{ 'events'|trans|capitalize }}</h2>
        <p>{{ 'all your upcomming events'|trans|capitalize }}</p>
        <div class="d-block">
            <div class="d-flex align-items-center pt-3 mr-5">
                <div class="icon icon-shape icon-sm icon-shape-secondary rounded mr-3"><span class="fas fa-calendar"></span></div>
                <div class="d-block">
                    <label class="mb-0">[[ event.name ]]</label>
                    <h4 class="mb-0">[[ event.date ]]</h4></div>
            </div>
        </div>
    </div>
</div>
